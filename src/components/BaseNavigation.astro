---
import { BRAND_SWITCH, NAV_CTA } from '../lib/navigation';

interface NavItem {
  text: string;
  href: string;
}

interface Props {
  brand: 'meta3' | 'genovate';
  currentPath: string;
  navItems: readonly NavItem[];
}

const { brand, currentPath, navItems } = Astro.props as Props;
const switchTo = BRAND_SWITCH[brand];
const cta = NAV_CTA[brand];
---

<div class="nav-wrapper">
  <nav class="nav-container" aria-label="Main Navigation">
    <div class="nav-content">
      <a href={brand === 'meta3' ? '/' : '/genovate'} class="logo" aria-label={brand === 'meta3' ? 'Meta3Ventures Home' : 'GenovateAI Home'}>
        {brand === 'meta3' ? (
          <span class="logo-text">Meta3<span class="gradient-text">Ventures</span></span>
        ) : (
          <span class="logo-text">Genovate<span class="gradient-text">AI</span></span>
        )}
      </a>
      <ul class="nav-items">
        {navItems.map((item) => (
          <li class="nav-item" key={item.href}>
            <a href={item.href} class={currentPath === item.href ? 'active' : ''}>
              {item.text}
            </a>
          </li>
        ))}
      </ul>
      <a href={cta.href} class="cta-button">
        {cta.text}
      </a>
      <a href={switchTo.href} class="switch-brand">
        {switchTo.text}
      </a>
    </div>
  </nav>
</div>

<style>
  .nav-wrapper {
    /* Add your styles here */
  }
  .nav-container {
    /* Add your styles here */
  }
  .nav-content {
    /* Add your styles here */
  }
  .logo {
    /* Add your styles here */
  }
  .logo-text {
    /* Add your styles here */
  }
  .gradient-text {
    /* Add your styles here */
  }
  .nav-items {
